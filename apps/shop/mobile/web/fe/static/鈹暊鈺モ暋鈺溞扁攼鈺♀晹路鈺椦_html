			   function activityWare(){
			    	$("#activityWare").hide();	
			    	$("#hzActivityHtml").hide();	
			    	
			    }    
			   
			  	$(function(){
			  		var url;
			 		if(isWeixin()){
			 			url = "/green_hlife/xnyx/wx/activity/query?tp=1";
			 		}else{
			 		 	url = "/green_hlife/xnyx/h5/activity/query?tp=1";	
			 		}
			 	   $.ajax({
			 	        url: url,
			 	        type:"POST",
			 	        dataType:"JSON",
			 	        success: function (data) {
			 	        	var result=data.result;
			 	        	var html="";
			 	        	if(data.isLogin&&!data.isReceive && data.result.isShow=="1"){
			 	        		$("#activityHtml").show();
			 	        	}else{
			 	        		isBuyHelpWare();
			 	        		$("#activityHtml").hide();
			 	        		
			 	        	}
			 	        	if(data.state){
			 	        		html+='<div class="activity_Ware" id="activityWare" >';
			 	        		html+='<a  href="/green_hlife/lvye/h5/sellerWare/activityWareDetail?id='+result.pId+'&activityId='+result.id+'">';
			 	        		html+='<img src="/green_hlife/resource/images/moisturizingwater.png" id="activtiyImg">';
			 	        		html+='</a>';
			 	        		html+='<div class="right">';
			 	        		html+='<p class="p1"><img id="activcloseImg" class="activcloseImg" src="/green_hlife/resource/images/xnyxGuide_close.png" onclick="activityWare();"></p>';
			 	        		html+='</div>';
			 	        		html+='</div>';
			 	        		$("#activityHtml").html(html);
			 	        	}else{
			 	        		 layer.open({
			 	    			    content:"获取活动商品失败！"
			 	    			    ,skin: 'msg'
			 	    			    ,time: 2 //2秒后自动关闭
			 	    			  });
			 	        	}
			 	        },
			 	        error: function (jqXHR, textStatus, errorThrown) {
			 	        }
			 	    });
			  		
			  	});
			    
			  	//判断是否购买矩阵商品
			  	function isBuyHelpWare(){
			  		var url;
			 		if(isWeixin()){
			 			url = "/green_hlife/xnyx/wx/center/isMyMatrix";
			 		}else{
			 		 	url = "/green_hlife/xnyx/h5/center/isMyMatrix";	
			 		}
			 		$.ajax({
			 	        url: url,
			 	        type:"POST",
			 	        dataType:"JSON",
			 	        success: function (data) {
			 	        	var result=data.flag;
			 	        	var html="";
			 	        	if(!data.flag){
			 	        		$("#hzActivityHtml").show();
			 	        	}else{
			 	        		$("#hzActivityHtml").hide();
			 	        		
			 	        	}
			 	        	if(data.state){
			 	        		html+='<div class="activity_Ware" id="activityWare" >';
			 	        		html+='<a  href="/green_hlife/lvye/h5/sellerWare/matrixWare">';
			 	        		html+='<img src="/green_hlife/resource/images/huzhutanchuang.png" id="activtiyImg">';
			 	        		html+='</a>';
			 	        		html+='<div class="right">';
			 	        		html+='<p class="p1"><img id="activcloseImg" class="activcloseImg" src="/green_hlife/resource/images/xnyxGuide_close.png" onclick="activityWare();"></p>';
			 	        		html+='</div>';
			 	        		html+='</div>';
			 	        		$("#hzActivityHtml").html(html);
			 	        	}
			 	        },
			 	        error: function (jqXHR, textStatus, errorThrown) {
			 	        }
			 	    });
			  		
			  	}
			  	
			  	function tween(){	
					var Tween = {
						"Elastic" : {					
							easeOut: function(t, b, c, d, a, p) {
								var s;
								if(t == 0) return b;
								if((t /= d) == 1) return b + c;
								if(typeof p == "undefined") p = d * .3;
								if(!a || a < Math.abs(c)) {
									a = c;
									s = p / 4;
								} else {
									s = p / (2 * Math.PI) * Math.asin(c / a);
								}
								return(a * Math.pow(2, -10 * t) * Math.sin((t * d - s) * (2 * Math.PI) / p) + c + b);
							}
						}
					};
						
					var ball = document.getElementById("offCanvasSideScroll")
					var elasticFall = function() {
						var start = 0,
							beginingValue = -40,
							changeValue =40,
							during = 100;
						var _run= function() {
							start++;
							var top = Tween.Elastic.easeOut(start, beginingValue, changeValue, during);
							ball.style.webkitTransform = "translateY("+top+"px)";
							if(start < during) requestAnimationFrame(_run);
						}
		                _run();
					};
					elasticFall();
				}
		</script>

	

