			 	   $.ajax({
			 	        url: url,
			 	        type:"POST",
			 	        dataType:"JSON",
			 	        success: function (data) {
			 	        	var rows=data.rows;
			 	        	if(data.state){
			 	        		var html = "";
			 	        		for(var i=0; i<rows.length; i++){
			 	        			html+='<li><span class="date"></span>';
			 	        			if(rows[i].title.length>15){
			 	        				html+='<a onclick="toBusinessTopic('+rows[i].id+')">'+rows[i].title.substring(0,15)+'</a>';
			 	        			}else{
			 	        				html+='<a onclick="toBusinessTopic('+rows[i].id+')">'+rows[i].title+'</a>';
			 	        			}
			 	        			html+='</li>';
			 	        		}
			 	        		$("#infoList").html(html);
			 				    startmarquee();   
			 	        	}else{
			 	        		$("#infoList").html('<p><i>加载推送话题失败</i></p>');
			 	        	}
			 	        },
			 	        error: function (jqXHR, textStatus, errorThrown) {
			 	        }
			 	    });
			    })
			    
