<!-- 调用百度地图来获取经纬度以及当前城市等-->
<script type="text/javascript" src="./绿业健康生活_files/api"></script><script type="text/javascript" src="./绿业健康生活_files/getscript"></script><link rel="stylesheet" type="text/css" href="./绿业健康生活_files/bmap.css">
<script type="text/javascript">
	var geoc = new BMap.Geocoder(); 	
    var geolocation = new BMap.Geolocation();
    
	BaiduMap = (function(){});
	
	/*获取当前经纬度*/
	BaiduMap.getCurrentPosition = function(successCallback,errorCallback){
		geolocation.getCurrentPosition(function(r){
			if(this.getStatus() == "0"){//获取当前经纬度成功
				var longitude = r.point.lng;//经度
				var latitude = r.point.lat;//纬度
				if(successCallback){
					successCallback(longitude,latitude);
				}
			}else{//获取当前经纬度失败
				if(errorCallback){
					errorCallback();
				}
			}
		});
	}
	
	/*获取当前城市名称（适用手机移动端）*/
	BaiduMap.getCurrentCity = function(callback){
		geolocation.getCurrentPosition(function(r){
			if(this.getStatus() == "0"){//获取当前位置成功
				var longitude = r.point.lng;//经度
				var latitude = r.point.lat;//纬度
				geoc.getLocation(r.point, function(rs){
					var addComp = rs.addressComponents;
					//alert(addComp.province + ", " + addComp.city + ", " + addComp.district + ", " + addComp.street + ", " + addComp.streetNumber);
					callback(addComp.city);
				});    
			}else{//根据ip地址获取城市
				var myCity = new BMap.LocalCity();
				myCity.get(myFun);
			    function myFun(result){
					var cityName = result.name;
					callback(cityName);
				}
			}
		});
	}
	
	/*根据ip获取当前城市名称（适用pc端）*/
	BaiduMap.getCurrentCityByIp = function(callback){
		var myCity = new BMap.LocalCity();
		myCity.get(myFun);
	    function myFun(result){
			var cityName = result.name;
			callback(cityName);
		}
	}
	
	/*根据地址名称获取经纬度*/
	BaiduMap.getPoint = function(address,successCallback,errorCallback){
		geoc.getPoint(address,function(point){
			if(point){
				var longitude = point.lng;//经度
				var latitude = point.lat;//纬度
				successCallback(longitude,latitude);
			}else{//地址没有解析到结果
				errorCallback();
			}
		});
	}
	
</script>
		<script type="text/javascript">
			var swiper = new Swiper('.swiper-container', {
		        pagination: '.swiper-pagination',
		        paginationClickable: true,
		        autoplay:3000
		    });
			
			$(function(){
				$(window).on('scroll',function() {
						console.log(123);
				});
				layui.use('flow', function(){
					  var $ = layui.jquery; //不用额外加载jQuery，flow模块本身是有依赖jQuery的，直接用即可。
					  var flow = layui.flow;
					  flow.load({
					    elem: '#rechargeList' //指定列表容器
					    ,done: function(page, next){ //到达临界点（默认滚动触发），触发下一页
					      	var lis = [];
					      	var url;
					    	if(isWeixin()){
					    		url = "/green_hlife/xnyx/wx/index/getIndexWare?pageIndex="+page+"&pageSize=10";
					    	}else{
					    	 	url = "/green_hlife/xnyx/h5/index/getIndexWare?pageIndex="+page+"&pageSize=10";
					    	}
					      //以jQuery的Ajax请求为例，请求下一页数据（注意：page是从2开始返回）
					      $.get(url, function(res){
					        //假设你的列表返回在data集合中
					        layui.each(res.data, function(index, item){	
					        	var ware=item.ware;
					        	for(var i=0;i<ware.length;i++){				        		
					        		lis.push('<div class="mui-card" onclick="toWareDetail(\''+ware[i].id+'\')">');
					        		lis.push('<div class="mui-card-media">');
					        		lis.push('<img src="http://112.74.38.131:8118/upload/'+ware[i].pic+'" class="media-img"/>');
					        		lis.push('</div>');
					        		lis.push('<div class="mui-card-content">');
					        		lis.push('<div class="mui-card-content-inner" style="padding:10px;">');
					        		lis.push('<p style="color:#333;margin-bottom: 0px;">'+ware[i].name+'<span class="mui-price">￥'+parseFloat(ware[i].price).toFixed(2)+'</span></p>');	
					        		lis.push('</div>');
					        		lis.push('</div>');
					        		lis.push('</div>');
						    
					        	}
					        }); 
					        
					        //执行下一页渲染，第二参数为：满足“加载更多”的条件，即后面仍有分页
					        //pages为Ajax返回的总页数，只有当前页小于总页数的情况下，才会继续出现加载更多
					        next(lis.join(''), page < res.totalPage);    
					      });
					    }
					  });
					});
				});

			
			function isWeixin() {
			    var ua = navigator.userAgent.toLowerCase();
			    if (ua.match(/MicroMessenger/i) == "micromessenger") {
			        return true;
			    } else {
			        return false;
			    }
			}
			
