<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="group.dny.api.mapper.RefundLogMapper">
    <insert id="insertRefundLog" parameterType="group.dny.api.entity.RefundLog">
        INSERT INTO RefundLog(RefundID, sWhoDo, StatusID, dNewDate, sMessage)
        VALUES (#{RefundID},
                #{sWhoDo},
                #{StatusID},
                #{dNewDate},
                #{sMessage})
    </insert>
</mapper>
